{
  "name": "template.rester",
  "version": "0.13.1",
  "description": "Rester Template.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "bash script/build.sh",
    "clean": "bash script/clean.sh",
    "lint": "eslint .",
    "pack": "npm run build && npm run push",
    "push": "bash script/push.sh",
    "start": "npm run start:dev",
    "start:debug": "cross-env MODE=DEV node --inspect-brk -r ts-node/register src/main/main.ts",
    "start:demo": "cross-env MODE=DEV node -r ts-node/register src/demo/main.ts",
    "start:dev": "cross-env MODE=DEV node -r ts-node/register src/main/main.ts",
    "start:prod": "npm run build && cross-env MODE=PROD node dist",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=src/test/jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register",
    "watch": "tsc -w"
  },
  "keywords": [
    "typescript",
    "rester",
    "backend"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://blog.don.red/)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@rester/core": "^0.13.1"
  },
  "devDependencies": {
    "@types/jasmine": "3.6.4",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.21.0",
    "jasmine": "3.6.4",
    "jasmine-console-reporter": "3.1.0",
    "ts-loader": "8.0.17",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack": "5.24.3",
    "webpack-cli": "4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/template.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/template/issues"
  },
  "homepage": "https://github.com/DevinDon/template/tree/typescript",
  "optionalDependencies": {
    "mongodb": "^3.6.4",
    "sqlite3": "^5.0.2"
  }
}
